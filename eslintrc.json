{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "parser": "babel-eslint",
  "rules": {
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "no-console": 0,
    "max-len": [
      1,
      500
    ],
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "consistent-return": 0,
    "guard-for-in": 0,
    "no-underscore-dangle": 0,
    "linebreak-style": "off",
    "no-return-assign": 0,
    "no-sequences": 0,
    "arrow-parens": 0,
    "no-unused-expressions": 0
  }
}
